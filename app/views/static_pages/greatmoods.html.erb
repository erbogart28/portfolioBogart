                        <div id="evaluation-home" class="tab-pane fade in active">
                          <h3 class="page-header">Homepage Evaluation | Alignment Analyzed</h3>
                            <div class="row row-flex" id="img-row">
                                <!-- image wrapped in anchor to allow for modal popup preview -->
                                <a href="#lightboxHome" class=" center-block" data-toggle="modal" id="eval-img"><%= image_tag('gmHomeEval.png', class: 'img-responsive', alt: 'GreatMoods homepage evaluation', title: 'Click to Expand' )%></a>
